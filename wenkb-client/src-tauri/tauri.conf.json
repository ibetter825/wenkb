{
  "productName": "wenkb-client",
  "version": "0.1.7",
  "identifier": "com.wenkb",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "WENKB",
        "width": 1366,
        "height": 768,
        "alwaysOnTop": false,
        "center": true,
        "visible": false,
        "label": "main",
        "decorations": false,
        "dragDropEnabled": false
      },
      {
        "fullscreen": false,
        "resizable": false,
        "title": "",
        "width": 200,
        "height": 80,
        "label": "splashscreen",
        "alwaysOnTop": true,
        "center": true,
        "visible": true,
        "decorations": false,
        "url": "./splashscreen.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "server/_internal/",
      "server/resources/",
      "server/wenkb-server.exe"
    ]
  }
}
